extends layout

block navigation
	header.navigation
		div.tablet_navigation
			p test
		div.logo
			p test
		div.basket
			p test
		nav
			ul.desktop
				li 
					a(href="#") home
				li 
					a(href="#") products
				li 
					a(href="#") services
				li.active 
					a(href="#") configure
				li
					a(href="#") contact

block picker
	section.picker
		nav
			ul.choose
				li.left
				li
					p choose package
				li.right
					a(href="#") >
		nav.items
			ul.menu_left
				- models.model.forEach(function(item,index) {
				li.item
					p= item.name
					img(src=item.thumbnail, alt="thumbnail")
				- })
				
block preview
	section.preview
		p this is the preview.